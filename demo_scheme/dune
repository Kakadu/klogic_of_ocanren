(rule
 (deps
  %{project_root}/bin/klogic_of_ocanren.exe
  %{cmo:../oleg_numbers}
  ;
  )
 (target OlegNumbers.scm)
 (mode promote)
 (action
  (run
   %{project_root}/bin/klogic_of_ocanren.exe
   ../.oleg_numbers.objs/byte/oleg_numbers.cmt
   -scheme
   -o
   %{target})))

(cram
 (applies_to oleg)
 (deps
  ./OlegNumbers.scm
  test_oleg.scm
  %{project_root}/faster-miniKanren/mk.scm
  %{project_root}/faster-miniKanren/mk-vicare.scm
  ;
  ))

